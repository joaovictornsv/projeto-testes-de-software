{
  "root": true,
  "env": { "node": true, "es2021": true },
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended"
  ],
  "ignorePatterns": [
    "dist", ".eslintrc.cjs"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins":[
    "prettier"
  ],
  "rules": {
    "prettier/prettier":"error"
  }
}
